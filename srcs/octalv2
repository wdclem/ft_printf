#include "../includes/ft_printf.h"

void	print_pointer(t_info *info)
{
long long	i;

	i = 0;
	i = change_size(info, i); /*make it a if */
	convert(info, i, 8);
	mod_init(info);
	min_width(info);
	print(info);
}	
